	<copyright>Copyright 2012 DtD Productions. All rights reserved.</copyright>
	<license>GNU General Public License version 2 or later; see LICENSE.txt</license>
	<authorEmail>info@dtdpro.com</authorEmail>
	<authorUrl>www.dtdpro.com</authorUrl>
	<version>1.1.0</version>
	<description>MOD_MUEUSERMENU_XML_DESCRIPTION</description>
	<files>
		<filename module="mod_mueusermenu">mod_mueusermenu.php</filename>
		<folder>tmpl</folder>
		<filename>index.html</filename>
		<filename>mod_mueusermenu.xml</filename>
	</files>
	<languages>
		<language tag="en-GB">en-GB.mod_mueusermenu.ini</language>
