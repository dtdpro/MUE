		<fields name="params">
			<fieldset name="basic">
				<field
					name="layout"
					type="modulelayout"
					label="JFIELD_ALT_LAYOUT_LABEL"
					description="JFIELD_ALT_MODULE_LAYOUT_DESC" />
				<field
					name="moduleclass_sfx"
					type="text"
					label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"
					description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />
				<field
					name="menu_class"
					type="text"
					default="menu menu-line"
					label="MOD_MUEUSERMENU_FIELD_MENUCLASS_LABEL"
					description="MOD_MUEUSERMENU_FIELD_MENUCLASS_DESC" />
			</fieldset>
		</fields>
	</config>
